        <splash height="960" src="res/screen/ios/FutbolCuatro-Daily-Fantasy-Manager-El-Juego-640x960.png" width="640" />
        <splash height="640" src="res/screen/ios/FutbolCuatro-Daily-Fantasy-Manager-El-Juego-640x960.png" width="960" />
        <splash height="1136" src="res/screen/ios/FutbolCuatro-Daily-Fantasy-Manager-El-Juego-640x1136.png" width="640" />
        <splash height="640" src="res/screen/ios/FutbolCuatro-Daily-Fantasy-Manager-El-Juego-640x1136.png" width="1136" />
        <splash height="1024" src="res/screen/ios/FutbolCuatro-Daily-Fantasy-Manager-El-Juego-768x1024.png" width="768" />
        <splash height="768" src="res/screen/ios/FutbolCuatro-Daily-Fantasy-Manager-El-Juego-1024x768.png" width="1024" />
        <splash height="2048" src="res/screen/ios/FutbolCuatro-Daily-Fantasy-Manager-El-Juego-1536x2048.png" width="1536" />
