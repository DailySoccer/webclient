    </platform>
    <plugin name="cc.fovea.cordova.purchase" spec="https://github.com/j3k0/cordova-plugin-purchase.git">
        <variable name="BILLING_KEY" value="" />
    </plugin>
    <plugin name="cordova-plugin-inappbrowser" version="1.2.1" />
    <plugin name="cordova-plugin-splashscreen" version="3.2.1" />
    <plugin name="cordova-plugin-console" version="1.0.2" />
    <plugin name="cordova-plugin-statusbar" version="2.1.1" />
    <plugin name="cordova-plugin-app-version" version="0.1.8" />
    <plugin name="cordova-plugin-device" version="1.1.1" />
    <preference name="SplashScreenDelay" value="20000" />
    <plugin name="com.comscoreplugin" spec="https://github.com/lnavarro/cordova-comscore-plugin.git" />
</widget>
